{
  "name": "bca-report2csv",
  "version": "1.0.0",
  "description": "convert bca report to csv",
  "scripts": {
    "build-osx": "electron-packager . Bca2Cvs --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "build-win": "electron-packager . Bca2Cvs --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=Paprika --version-string.FileDescription=CE --version-string.ProductName='Bca2Cvs'"
  },
  "main": "app.js",
  "author": "Jemmy Phan <jemmyphan94@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "2.0.6",
    "electron-packager": "12.1.0"
  }
}
